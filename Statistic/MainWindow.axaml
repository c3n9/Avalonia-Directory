		xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Statistic.MainWindow"
        Title="Statistic">
	<!--<Window.DataContext>
		<local:MainWindow/>
	</Window.DataContext>-->
	<Grid>
		<lvc:CartesianChart Series="{Binding Series}" x:DataType="local:MainWindow">
		</lvc:CartesianChart>
	</Grid>
</Window>
